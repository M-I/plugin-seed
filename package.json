{
  "name": "pouchdb-plugin-seed",
  "version": "0.1.0",
  "description": "PouchDB Plugin Seed project",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pouchdb/plugin-seed.git"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "couch",
    "couchdb"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pouchdb/plugin-seed/issues"
  },
  "scripts": {
    "prepublish": "npm run build",
    "test-node": "mocha test/test.js",
    "test-browser": "zuul --no-coverage --phantom --ui mocha-bdd test/test.js",
    "test-local": "zuul --no-coverage --local 3000 --ui mocha-bdd test/test.js",
    "lint": "jshint -c .jshintrc lib test",
    "test": "npm run lint && npm run test-node",
    "build": "rimraf dist && mkdirp dist && npm run browserify && npm run min",
    "browserify": "browserify . -g es3ify | derequire > dist/pouchdb.mypluginname.js",
    "min": "uglifyjs dist/pouchdb.mypluginname.js -mc > dist/pouchdb.mypluginname.min.js",
    "test-coverage": "istanbul test node_modules/mocha/bin/_mocha test/test.js --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
  },
  "dependencies": {
    "pouchdb-utils": "5.4.0"
  },
  "devDependencies": {
    "browserify": "^12.0.2",
    "chai": "^3.5.0",
    "derequire": "^2.0.0",
    "es3ify": "^0.1.3",
    "istanbul": "^0.2.7",
    "jshint": "2.8.0",
    "mkdirp": "^0.5.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "pouchdb-memory": "^1.1.0",
    "request": "^2.36.0",
    "sauce-connect-launcher": "^0.4.2",
    "selenium-standalone": "^5.0.0",
    "uglify-js": "^2.4.13",
    "zuul": "^3.10.1"
  },
  "files": ["lib", "dist"]
}
